# Root-level ignores
node_modules/
*.log
.DS_Store
Thumbs.db

# Ignore environment files
.env
.env.local
.env.development.local
.env.production.local
.env.test.local

# Temporary and cache files
.tmp/
.cache/

# Ignore package manager lock files
yarn.lock
pnpm-lock.yaml
package-lock.json

# Editor-specific files
.vscode/
.idea/
*.suo
*.swp

# Lint and formatter caches
.eslintcache
.prettierignore

# Logs and debugging files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Client-side (Next.js) specific ignores
client/.next/
client/out/
client/build/
client/public/static/

# Server-side (Express.js) specific ignores
server/dist/
server/build/
server/coverage/
server/.nyc_output/
